    <record id="delivery_carrier_usps_first" model="delivery.carrier">
        <field name="name">USPS - First Class</field>
        <field name="easypost_service">First</field>
        <field name="partner_id" ref="res_partner_usps" />
        <field name="delivery_type">easypost</field>
        <field name="integration_level">rate_and_ship</field>
        <field name="prod_environment" eval="True" />
